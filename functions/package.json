{
  "name": "hunar-agent-creator-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "functions-framework --target=createAgent",
    "predeploy": "npm run build",
    "deploy:functions": "gcloud functions deploy createAgent --runtime nodejs20 --trigger-http --allow-unauthenticated",
    "deploy:run": "gcloud run deploy hunar-agent-creator-service --source . --platform managed --allow-unauthenticated"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "typescript": "^5.0.0"
  }
}
